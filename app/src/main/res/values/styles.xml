<resources>


    <style name="Theme.Codejump" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="actionBarItemBackground">@drawable/selectable_background_codejump</item>
        <item name="popupMenuStyle">@style/PopupMenu.Codejump</item>
        <item name="dropDownListViewStyle">@style/DropDownListView.Codejump</item>
        <item name="actionBarTabStyle">@style/ActionBarTabStyle.Codejump</item>
        <item name="actionDropDownStyle">@style/DropDownNav.Codejump</item>
        <item name="actionBarStyle">@style/ActionBar.Solid.Codejump</item>
        <item name="actionModeBackground">@drawable/cab_background_top_codejump</item>
        <item name="actionModeSplitBackground">@drawable/cab_background_bottom_codejump</item>
        <item name="actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Codejump</item>


    </style>

    <style name="ActionBar.Solid.Codejump" parent="@style/Widget.AppCompat.Light.ActionBar.Solid">
        <item name="background">@drawable/ab_solid_codejump</item>
        <item name="backgroundStacked">@drawable/ab_stacked_solid_codejump</item>
        <item name="backgroundSplit">@drawable/ab_bottom_solid_codejump</item>
        <item name="progressBarStyle">@style/ProgressBar.Codejump</item>
        <item name="titleTextStyle">@style/CodeJump.ActionBar.TitleTextStyle</item>
    </style>

    <style name="CodeJump.ActionBar.TitleTextStyle" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title" >
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ActionBar.Transparent.Codejump" parent="@style/Widget.AppCompat.Light.ActionBar">
        <item name="background">@drawable/ab_transparent_codejump</item>
        <item name="progressBarStyle">@style/ProgressBar.Codejump</item>
    </style>

    <style name="PopupMenu.Codejump" parent="@style/Widget.AppCompat.Light.PopupMenu">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_codejump</item>
    </style>

    <style name="DropDownListView.Codejump" parent="@style/Widget.AppCompat.Light.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_codejump</item>
    </style>

    <style name="ActionBarTabStyle.Codejump" parent="@style/Widget.AppCompat.Light.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_codejump</item>
    </style>

    <style name="DropDownNav.Codejump" parent="@style/Widget.AppCompat.Light.Spinner.DropDown.ActionBar">
        <item name="android:background">@drawable/spinner_background_ab_codejump</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_codejump</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_codejump</item>
    </style>

    <style name="ProgressBar.Codejump" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_codejump</item>
    </style>

    <style name="ActionButton.CloseMode.Codejump" parent="@style/Widget.AppCompat.Light.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_codejump</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Codejump.Widget" parent="@style/Theme.AppCompat">
        <item name="popupMenuStyle">@style/PopupMenu.Codejump</item>
        <item name="dropDownListViewStyle">@style/DropDownListView.Codejump</item>
    </style>

    <style name="EditTextcodejump" parent="android:Widget.EditText">
        <item name="android:background">@drawable/codejump_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="AutoCompleteTextViewcodejump" parent="android:Widget.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/codejump_list_selector_holo_light</item>
        <item name="android:background">@drawable/codejump_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="CheckBoxcodejump" parent="android:Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/codejump_btn_check_holo_light</item>
    </style>

    <style name="RadioButtoncodejump" parent="android:Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/codejump_btn_radio_holo_light</item>
    </style>

    <style name="Buttoncodejump" parent="android:Widget.Button">
        <item name="android:background">@drawable/codejump_btn_default_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ImageButtoncodejump" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/codejump_btn_default_holo_light</item>
    </style>

    <style name="Spinnercodejump" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/codejump_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/codejump_list_selector_holo_light</item>
    </style>

    <style name="SpinnerDropDownItemcodejump" parent="android:Widget.DropDownItem.Spinner">
        <item name="android:checkMark">@drawable/codejump_btn_radio_holo_light</item>
    </style>
    <style name="Tabcodejump">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        <item name="android:background">@drawable/codejump_tab_indicator_holo</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minWidth">80dip</item>
    </style>

    <style name="TabTextcodejump">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <!-- v14 <item name="android:textAllCaps">true</item> -->
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">2</item>
        <item name="android:maxWidth">180dip</item>
    </style>

    <style name="ProgressBarcodejump" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/codejump_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/codejump_progress_indeterminate_horizontal_holo_light</item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>
    </style>

    <style name="SeekBarcodejump" parent="android:Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/codejump_scrubber_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/codejump_scrubber_progress_horizontal_holo_light</item>
        <item name="android:minHeight">13dip</item>
        <item name="android:maxHeight">13dip</item>
        <item name="android:thumb">@drawable/codejump_scrubber_control_selector_holo_light</item>
        <item name="android:thumbOffset">16dip</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
    </style>

    <style name="RatingBarcodejump" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/codejump_ratingbar_full_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/codejump_ratingbar_full_holo_light</item>
    </style>

    <style name="RatingBarBigcodejump">
        <item name="android:progressDrawable">@drawable/codejump_ratingbar_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/codejump_ratingbar_holo_light</item>
        <item name="android:minHeight">35dip</item>
        <item name="android:maxHeight">35dip</item>
    </style>

    <style name="RatingBarSmallcodejump">
        <item name="android:progressDrawable">@drawable/codejump_ratingbar_small_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/codejump_ratingbar_small_holo_light</item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>
    </style>

    <style name="Togglecodejump" parent="android:Widget.Button.Toggle">
        <item name="android:background">@drawable/codejump_btn_toggle_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ListViewcodejump" parent="android:Widget.ListView">
        <item name="android:listSelector">@drawable/codejump_list_selector_holo_light</item>
    </style>

    <style name="ListViewcodejump.White" parent="android:Widget.ListView.White">
        <item name="android:listSelector">@drawable/codejump_list_selector_holo_light</item>
    </style>

    <style name="SpinnerItemcodejump" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="FloatLayout">
        <item name="android:textColor">@color/codejump_color</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ButtonFacebook" parent="android:Widget.Button">
        <item name="android:background">@drawable/facebook_btn_default_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="ImageButtonFacebook" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/facebook_btn_default_holo_light</item>
    </style>


</resources>

